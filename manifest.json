{
    "name": "subjects",
    "description": "Allows registration and management of optional subjects.",
    "version": "0.0.1",
    "min_base_version": "0.2.8",
    "locale": {
        "name": {
            "cs": "Volitelné předměty",
            "en": "Optional subjects"
        }
    },
    "schema": {
        "settings": {
            "active_version": "INTEGER NULL",
            "open_editing": "INTEGER",
            "open_accepting": "INTEGER"
        },
        "versions": {
            "id": "INTEGER PRIMARY KEY",
            "name": "TEXT"
        },
        "subjects": {
            "id": "INTEGER PRIMARY KEY",
            "code": "INTEGER",
            "name": "TEXT",
            "annotaion": "TEXT NULL",
            "version": "INTEGER"
        },
        "main": {
            "id": "INTEGER PRIMARY KEY",
            "user": "INTEGER",
            "created": "INTEGER",
            "state": "INTEGER DEFAULT 0",
            "version": "INTEGER"
        },
        "lists": {
            "list": "INTEGER",
            "subject": "INTEGER",
            "level": "INTEGER DEFAULT 0"
        }
    }
}
